{
    "name": "suzonraj/zipper",
    "type": "library",
    "description": "Wrapper for chumper/zipper - This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "http://github.com/suzonraj/zipper",
    "authors": [
        {
            "name": "Almas MD. Estiak",
            "email": "almas.estiak@gmail.com",
            "homepage": "http://w3sbd.com",
            "role": "Developer"
        },
        {
            "name": "Nils Plaschke",
            "email": "github@nilsplaschke.de",
            "homepage": "http://nilsplaschke.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-zip": "*",
        "illuminate/support": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*",
        "illuminate/filesystem": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Chumper\\Zipper\\": "src/Chumper/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chumper\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chumper\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Chumper\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
